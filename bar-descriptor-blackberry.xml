<?xml version='1.0' encoding='utf-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.kdab.flow-pomodoro</id>
    <name>flow-pomodoro</name>
    <versionNumber>1.0.0</versionNumber>
    <description>flow pomodoro</description>
    <copyright>Sergio Martins</copyright>
    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
        <autoOrients>true</autoOrients>
        <aspectRatio>landscape</aspectRatio>
    </initialWindow>    
    <env var="LD_LIBRARY_PATH" value="app/native/runtime/qt/lib:$LD_LIBRARY_PATH"/>
    <env var="QT_PLUGIN_PATH" value="app/native/runtime/qt/plugins:$QT_PLUGIN_PATH"/>
    <env var="QML_IMPORT_PATH" value="app/native/runtime/qt/imports:$QML_IMPORT_PATH"/>
    <env var="QML2_IMPORT_PATH" value="app/native/runtime/qt/qml:$QML2_IMPORT_PATH"/>
    
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Core.so.5">runtime/qt/lib/libQt5Core.so.5</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Gui.so.5">runtime/qt/lib/libQt5Gui.so.5</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Qml.so.5">runtime/qt/lib/libQt5Qml.so.5</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Quick.so.5">runtime/qt/lib/libQt5Quick.so.5</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Network.so.5">runtime/qt/lib/libQt5Network.so.5</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/lib/libQt5Xml.so.5">runtime/qt/lib/libQt5Xml.so.5</asset>
    
    <asset path="/data/extra2/installation/qt/blackberry-5.3/plugins">runtime/qt/plugins</asset>
    <asset path="/data/extra2/installation/qt/blackberry-5.3/qml">runtime/qt/qml</asset>
    
    <asset path="../src/img/icon.png">icon.png</asset>
    <icon>
        <image>icon.png</image>
    </icon>

    <action system="true">run_native</action>
    <asset path="flow" entry="true" type="Qnx/Elf">src</asset>
    <author>KDAB</author>
    <authorId>gYAAgNgXj6_2fDG0Fp5jVVfRWGg</authorId>
</qnx>
